<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets">
    <Import Project="gtksourceview-build-defines.props" />
  </ImportGroup>
  <PropertyGroup Label="UserMacros">
    <GtkSourceviewDoInstall>
mkdir $(CopyDir)
mkdir $(CopyDir)\bin
copy $(SolutionDir)$(Configuration)\$(Platform)\bin\$(GtkSourceviewDllPrefix)gtksourceview$(GtkSourceviewDllSuffix).dll $(CopyDir)\bin
copy $(SolutionDir)$(Configuration)\$(Platform)\bin\$(GtkSourceviewDllPrefix)gtksourceview$(GtkSourceviewDllSuffix).pdb $(CopyDir)\bin
mkdir $(CopyDir)\include\gtksourceview-$(ApiVersion)\gtksourceview\completion-providers\words
copy ..\..\gtksourceview\gtksource.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksource.h
copy ..\..\gtksourceview\gtksourceautocleanups.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourceautocleanups.h
copy ..\..\gtksourceview\gtksourcebuffer.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcebuffer.h
copy ..\..\gtksourceview\gtksourcecompletion.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcecompletion.h
copy ..\..\gtksourceview\gtksourcecompletioncontext.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcecompletioncontext.h
copy ..\..\gtksourceview\gtksourcecompletioninfo.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcecompletioninfo.h
copy ..\..\gtksourceview\gtksourcecompletionitem.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcecompletionitem.h
copy ..\..\gtksourceview\gtksourcecompletionproposal.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcecompletionproposal.h
copy ..\..\gtksourceview\gtksourcecompletionprovider.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcecompletionprovider.h
copy ..\..\gtksourceview\gtksourceencoding.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourceencoding.h
copy ..\..\gtksourceview\gtksourcefile.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcefile.h
copy ..\..\gtksourceview\gtksourcefileloader.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcefileloader.h
copy ..\..\gtksourceview\gtksourcefilesaver.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcefilesaver.h
copy ..\..\gtksourceview\gtksourcegutter.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcegutter.h
copy ..\..\gtksourceview\gtksourcegutterrenderer.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcegutterrenderer.h
copy ..\..\gtksourceview\gtksourcegutterrendererpixbuf.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcegutterrendererpixbuf.h
copy ..\..\gtksourceview\gtksourcegutterrenderertext.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcegutterrenderertext.h
copy ..\..\gtksourceview\gtksourcelanguage.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcelanguage.h
copy ..\..\gtksourceview\gtksourcelanguagemanager.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcelanguagemanager.h
copy ..\..\gtksourceview\gtksourcemap.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcemap.h
copy ..\..\gtksourceview\gtksourcemark.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcemark.h
copy ..\..\gtksourceview\gtksourcemarkattributes.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcemarkattributes.h
copy ..\..\gtksourceview\gtksourceprintcompositor.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourceprintcompositor.h
copy ..\..\gtksourceview\gtksourceregion.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourceregion.h
copy ..\..\gtksourceview\gtksourcesearchcontext.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcesearchcontext.h
copy ..\..\gtksourceview\gtksourcesearchsettings.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcesearchsettings.h
copy ..\..\gtksourceview\gtksourcespacedrawer.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcespacedrawer.h
copy ..\..\gtksourceview\gtksourcestyle.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcestyle.h
copy ..\..\gtksourceview\gtksourcestylescheme.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcestylescheme.h
copy ..\..\gtksourceview\gtksourcestyleschemechooser.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcestyleschemechooser.h
copy ..\..\gtksourceview\gtksourcestyleschemechooserbutton.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcestyleschemechooserbutton.h
copy ..\..\gtksourceview\gtksourcestyleschemechooserwidget.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcestyleschemechooserwidget.h
copy ..\..\gtksourceview\gtksourcestyleschememanager.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcestyleschememanager.h
copy ..\..\gtksourceview\gtksourcetag.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcetag.h
copy ..\..\gtksourceview\gtksourcetypes.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourcetypes.h
copy ..\..\gtksourceview\gtksourceundomanager.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourceundomanager.h
copy ..\..\gtksourceview\gtksourceutils.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourceutils.h
copy ..\..\gtksourceview\gtksourceview.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourceview.h
copy ..\..\gtksourceview\gtksourceview-typebuiltins.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourceview-typebuiltins.h
copy ..\..\gtksourceview\gtksourceversion.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourceversion.h
copy ..\..\gtksourceview\gtksourceview-enumtypes.h $(CopyDir)\include\gtksourceview-3.0\gtksourceview\gtksourceview-enumtypes.h
copy ..\..\gtksourceview\completion-providers\words\gtksourcecompletionwords.h $(CopyDir)\include\gtksourceview-$(ApiVersion)\gtksourceview\completion-providers\words
mkdir $(CopyDir)\lib
copy $(SolutionDir)$(Configuration)\$(Platform)\bin\gtksourceview-$(ApiVersion).lib $(CopyDir)\lib
copy ..\gtksourceview-$(ApiVersion).pc $(CopyDir)\lib\pkgconfig
mkdir $(CopyDir)\share\gtksourceview-$(ApiVersion)\language-specs
copy ..\..\data\language-specs\*.lang $(CopyDir)\share\gtksourceview-$(ApiVersion)\language-specs
copy ..\..\data\language-specs\*.rng $(CopyDir)\share\gtksourceview-$(ApiVersion)\language-specs
copy ..\..\data\language-specs\check-language.sh $(CopyDir)\share\gtksourceview-$(ApiVersion)\language-specs
copy ..\..\data\language-specs\language.dtd $(CopyDir)\share\gtksourceview-$(ApiVersion)\language-specs
mkdir $(CopyDir)\share\gtksourceview-$(ApiVersion)\styles
copy ..\..\data\styles\*.xml $(CopyDir)\share\gtksourceview-$(ApiVersion)\styles
copy ..\..\data\styles\styles.rng $(CopyDir)\share\gtksourceview-$(ApiVersion)\styles
copy ..\..\data\styles\check-style.sh $(CopyDir)\share\gtksourceview-$(ApiVersion)\styles
    </GtkSourceviewDoInstall>
    <GtkSourceviewPCFiles>..\gtksourceview-$(ApiVersion).pc</GtkSourceviewPCFiles>
    <GtkSourceviewGenPC>(if not exist $(CopyDir)\lib\pkgconfig mkdir $(CopyDir)\lib\pkgconfig) &amp; ($(PythonDir)\python ..\gtksourceviewpc.py --prefix=$(CopyDir) --version=$(GtkSourceviewMajorVersion).$(GtkSourceviewMinorVersion).$(GtkSourceviewMicroVersion))</GtkSourceviewGenPC>
    <GtkSourceviewGenPCX64>(if not exist $(CopyDir)\lib\pkgconfig mkdir $(CopyDir)\lib\pkgconfig) &amp; ($(PythonDirX64)\python ..\gtksourceviewpc.py --prefix=$(CopyDir) --version=$(GtkSourceviewMajorVersion).$(GtkSourceviewMinorVersion).$(GtkSourceviewMicroVersion))</GtkSourceviewGenPCX64>
  </PropertyGroup>
  <PropertyGroup>
    <_PropertySheetDisplayName>gtksourceviewinstallprops</_PropertySheetDisplayName>
    <OutDir>$(SolutionDir)$(Configuration)\$(PlatformName)\bin\</OutDir>
    <IntDir>$(SolutionDir)$(Configuration)\$(PlatformName)\obj\$(ProjectName)\</IntDir>
  </PropertyGroup>
  <ItemGroup>
    <BuildMacro Include="GtkSourceviewDoInstall">
      <Value>$(GtkSourceviewDoInstall)</Value>
    </BuildMacro>
    <BuildMacro Include="GtkSourceviewPCFiles">
      <Value>$(GtkSourceviewPCFiles)</Value>
    </BuildMacro>
    <BuildMacro Include="GtkSourceviewGenPC">
      <Value>$(GtkSourceviewGenPC)</Value>
    </BuildMacro>
    <BuildMacro Include="GtkSourceviewGenPCX64">
      <Value>$(GtkSourceviewGenPCX64)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>
